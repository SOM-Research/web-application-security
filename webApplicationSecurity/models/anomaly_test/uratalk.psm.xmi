<?xml version="1.0" encoding="ISO-8859-1"?>
<servletSecurity:Security xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:servletSecurity="http://www.xtext.org/servletSecurity/ServletSecurity">
  <constraint>
    <webResourceCollection>
      <urlPattern value="/slim3/gtx"/>
    </webResourceCollection>
    <authConstraint>
      <roleNameTxt>admin</roleNameTxt>
    </authConstraint>
  </constraint>
  <constraint>
    <webResourceCollection>
      <urlPattern value="/my/*"/>
    </webResourceCollection>
    <authConstraint>
      <roleNameTxt>*</roleNameTxt>
    </authConstraint>
  </constraint>
  <constraint>
    <webResourceCollection>
      <urlPattern value="/cron/*"/>
    </webResourceCollection>
    <authConstraint>
      <roleNameTxt>admin</roleNameTxt>
    </authConstraint>
  </constraint>
  <constraint>
    <webResourceCollection>
      <urlPattern value="/task/*"/>
    </webResourceCollection>
    <authConstraint>
      <roleNameTxt>admin</roleNameTxt>
    </authConstraint>
  </constraint>
  <constraint>
    <webResourceCollection>
      <urlPattern value="/templates/*"/>
    </webResourceCollection>
    <authConstraint>
      <roleNameTxt>admin</roleNameTxt>
    </authConstraint>
  </constraint>
</servletSecurity:Security>
