<?xml version="1.0" encoding="ISO-8859-1"?>
<ejbSecurity:Security xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ejbSecurity="http://www.xtext.org/ejbSecurity/EjbSecurity">
  <enterpriseBeans>
    <session>
      <ejbName name="DDBasedSLSB"/>
      <ejbClass name="org.jboss.as.test.integration.ejb.security.DDBasedSLSB"/>
      <securityRoleRef>
        <roleNameLink name="DDRole1"/>
      </securityRoleRef>
      <securityRoleRef>
        <roleNameLink name="DDRole2"/>
      </securityRoleRef>
      <securityRoleRef>
        <roleNameLink name="DDRole3"/>
      </securityRoleRef>
      <securityIdentity>
        <userCallerIdentity/>
      </securityIdentity>
    </session>
  </enterpriseBeans>
  <assemblyDescriptor>
    <methodPermission>
      <method ejbName="//@enterpriseBeans/@session.0/@ejbName" methodName="onlyTestRoleCanAccess"/>
    </methodPermission>
    <excludeList>
      <method ejbName="//@enterpriseBeans/@session.0/@ejbName" methodName="accessDenied"/>
    </excludeList>
  </assemblyDescriptor>
</ejbSecurity:Security>
